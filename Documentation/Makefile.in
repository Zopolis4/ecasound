	cp $(srcdir)/qtecasound.1 $(mandir)/man1
	cp $(srcdir)/ecatools.1 $(mandir)/man1
	cp $(srcdir)/ecasound-iam.1 $(mandir)/man1
	cp $(srcdir)/ecasoundrc.5 $(mandir)/man5
	cp $(srcdir)/ecasound_faq.txt ../$(topdir)/FAQ

